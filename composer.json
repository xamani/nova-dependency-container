{
    "name": "xamani/nova-dependency-container",
    "description": "A Laravel Nova form container for grouping fields that depend on other field values.",
    "keywords": [
        "laravel",
        "nova",
        "nova-4",
        "nova-5",
        "field"
    ],
    "authors": [
        {
            "name": "Hamed Xamani",
            "email": "zamani1773@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Xamani\\DependencyContainer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xamani\\DependencyContainer\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
